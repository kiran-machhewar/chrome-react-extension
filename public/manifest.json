{
  "manifest_version": 2,
  "name": "React Content Script",
  "version": "0.1",
  "description": "React Content Script",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}